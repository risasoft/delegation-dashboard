{
  "name": "@risasoft/delegation-dashboard",
  "description": "Risa Staking",
  "version": "0.1.2",
  "author": "@risa_staking",
  "license": "GPL-3.0-or-later",
  "repository": "@risasoft/delegation-dashboard",
  "type": "module",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "bun run public/index.html",
    "start-testnet": "yarn copy-testnet-config && yarn start",
    "build": "bun build ./src/index.tsx --outdir ./dist",
    "build-testnet": "yarn copy-testnet-config && bun run build",
    "build-mainnet": "yarn copy-mainnet-config && bun run build",
    "test": "node scripts/test.js",
    "copy-wasm": "mkdir dist && cp ./public/bls_c.wasm ./dist && cp ./public/.htaccess ./dist && cp ./public/_redirects ./dist",
    "copy-testnet-config": "yarn copy-wasm && cp ./src/config.testnet.ts ./src/config.ts",
    "copy-mainnet-config": "yarn copy-wasm && cp ./src/config.mainnet.ts ./src/config.ts",
    "clean": "rm -rf ./dist",
    "lint": "eslint --ext js,ts,tsx src --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [
      "/src"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@ledgerhq/devices": "^8.0.7",
    "@multiversx/sdk-core": "12.6.1",
    "@multiversx/sdk-dapp": "2.19.2",
    "@multiversx/sdk-network-providers": "1.5.0",
    "@multiversx/sdk-wallet": "4.2.0",
    "@parcel/config-default": "^2.9.3",
    "@parcel/core": "^2.9.3",
    "@popperjs/core": "^2.11.8",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.8.3",
    "@types/jest": "26.0.24",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-router-dom": "5.3.3",
    "axios": "1.3.5",
    "bignumber.js": "^9.1.1",
    "bootstrap": "5.3.1",
    "buffer": "^5.5.0",
    "bun": "^1.0.0",
    "dayjs": "1.11.7",
    "formik": "2.2.9",
    "lightningcss": "^1.19.0",
    "moment": "2.29.4",
    "parcel": "2.9.3",
    "react": "18.2.0",
    "react-bootstrap": "2.8.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-router-dom": "6.8.0",
    "redux": "4.2.1",
    "sass": "1.63.2",
    "sharp": "0.31.1",
    "shx": "0.3.4",
    "typescript": "5.0.3",
    "web-vitals": "3.3.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.9.3",
    "@parcel/transformer-sass": "2.9.3",
    "@parcel/transformer-webmanifest": "2.9.3",
    "@types/node": "^18.15.13",
    "@types/qrcode": "1.5.0",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "crypto-browserify": "3.12.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-react": "1.1.7",
    "eslint-config-react-app": "7.0.1",
    "eslint-config-standard": "17.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "events": "^3.1.0",
    "path-browserify": "^1.0.0",
    "prettier": "2.7.1",
    "process": "0.11.10",
    "punycode": "^1.4.1",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0"
  }
}
